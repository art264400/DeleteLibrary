@model DeleteLibrary.Models.Book
@using (Html.BeginForm())
{
@Html.HiddenFor(m => m.Id)
<div>
    <div>
        @Html.LabelFor(m => m.Author)
        @Html.EditorFor(m => m.Author)
        @Html.ValidationMessageFor(m => m.Author)
    </div>
    <div>
        @Html.LabelFor(m => m.Name)
        @Html.EditorFor(m => m.Name)
        @Html.ValidationMessageFor(m => m.Name)
    </div>
    <div>
        @Html.LabelFor(m => m.Discription)
        @Html.EditorFor(m => m.Discription)
        @Html.ValidationMessageFor(m => m.Discription)
    </div>
    <div>
        @Html.LabelFor(m => m.Cover)
        @Html.EditorFor(m => m.Cover)
        @Html.ValidationMessageFor(m => m.Cover)
    </div>
    <div>
        <input type="submit" value="Сохранить"/>
    </div>
</div>
}

